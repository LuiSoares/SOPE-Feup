#include <dirent.h>

int main()
{

char[] testDirectory = "/usr/users2/mieic2014/up201403526/Downloads";	// test direcotry

DIR *dir;
struct dirent *ent;
if ((dir = opendir (testDirectory)) != NULL) {
  /* print all the files and directories within directory */
  while ((ent = readdir (dir)) != NULL) {
    printf ("%s\n", ent->d_name);
  }
  closedir (dir);
} else {
  /* could not open directory */
  perror ("");
  return EXIT_FAILURE;
}
 
return 0;
}
